class Solution:
    def findChampion(self, grid: List[List[int]) -> int:
        n = len(grid)
        for i in range(n):
            isChampion = True
            for j in range(n):
                if i != j and grid[j][i] == 1:
                    isChampion = False
                    break
            if isChampion:
                return i
        return -1
